{
    "name": "vursion/laravel-sitemappable",
    "description": "laravel-sitemappable",
    "keywords": [
        "vursion",
        "laravel",
        "sitemap"
    ],
    "homepage": "https://github.com/vursion/laravel-sitemappable",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jochen Sengier",
            "email": "support@vursion.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1 || ^7.2 || ^7.3 || ^7.4 || ^8.0 || ^8.1"
    },
    "require-dev": {
        "orchestra/testbench": "^3.0 || ^4.0 || ^5.0 || ^6.0 || ^7.0",
        "phpunit/phpunit": "^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Vursion\\LaravelSitemappable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vursion\\LaravelSitemappable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vursion\\LaravelSitemappable\\SitemappableServiceProvider"
            ]
        }
    }
}
